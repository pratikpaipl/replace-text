{
    "name": "pifunder",
    "version": "0.0.1",
    "author": "Ionic Framework",
    "homepage": "https://ionicframework.com/",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "postinstall": "ngcc",
        "serve-local:admin": "ng serve --project=admin --port 8004 -c=local",
        "serve-local:front": "ng serve --project=front --port 8005 -c=local",
        "serve-dev:admin": "ng serve --project=admin --port 8104 -c=dev",
        "serve-dev:front": "ng serve --project=front --port 8105 -c=dev",
        "serve-beta:admin": "ng serve --project=admin --port 8104 -c=beta",
        "serve-beta:front": "ng serve --project=front --port 8105 -c=beta",
        "dev:ssr": "ng run front:serve-ssr",
        "front-dev-build:ssr": "ng build --project=front -c=dev && ng run front:server",
        "front-beta-build:ssr": "ng build --project=front -c=beta && ng run front:server",
        "front-dev-serve:ssr": "ng serve --project=front -c=dev --port 8105 && ng run front:server",
        "front-beta-serve:ssr": "ng serve --project=front -c=beta --port 8105 && ng run front:server",
        "serve:ssr": "node front/server/main.js",
        "browser-dev:admin": "ng build --project=admin -c=dev",
        "browser-beta:admin": "ng build --project=admin -c=beta",

        "front-prod-build:ssr": "ng build --project=front --configuration production && ng run front:server",
        "admin-prod-build": "ng build --project=admin --configuration production",
        
        "android:admin": "ionic cordova run android --project=admin",
        "bandroid:admin": "sudo ionic cordova platform add android@10.1.1 --project=admin",
        "bios:admin": "sudo ionic cordova  build ios --project=admin",
        "browser:all": "ng build --project=admin && ng build --project=front"
    },
    "private": true,
